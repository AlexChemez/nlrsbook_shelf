<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/nlrsbook_shelf/db" VERSION="20200406" COMMENT="XMLDB file for Moodle nlrsbook_shelf"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="nlrsbook_shelf" COMMENT="Stores the nlrsbook activity module instances.">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="user_id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="User ID"/>
        <FIELD NAME="token" TYPE="text" LENGTH="" NOTNULL="true" SEQUENCE="false" COMMENT="User token"/>
        <FIELD NAME="datetime" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="The name of the activity module instance"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
